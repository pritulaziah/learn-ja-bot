{
  "name": "learn-ja-bot",
  "version": "1.0.0",
  "description": "Bot for learning japanese language",
  "engines": {
    "node": ">= 18.12 <19"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --max-warnings 0 --ext .ts,.mjs ./src",
    "build": "tsc -p tsconfig.json",
    "dev": "tsc-watch --onSuccess \"node build/index.js\"",
    "start": "node build/index.js"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.json prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pritulaziah/learn-ja-bot.git"
  },
  "author": "ziahpritula <ziahherzen@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pritulaziah/learn-ja-bot/issues"
  },
  "homepage": "https://github.com/pritulaziah/learn-ja-bot#readme",
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "dotenv": "^16.0.3",
    "grammy": "^1.16.0"
  },
  "devDependencies": {
    "@grammyjs/types": "^3.1.1",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "prisma": "^4.13.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.0.4"
  }
}
